#client
  #connection-manager
    .username
      p.label Username:
      input(type:'text')
    .password
      p.label Password:
      input(type:'password')
    a.connect-btn(href:"#") Connect
#canvas
  #left
    .status-wrapper
      span.left
      #status-box
        .status-icon
        .my-info
          p.name la cuturubrita loca
          p.message pam pararam
          a.status(href:"#")  
            span.label available
        .photo
          img(src:'/images/photo.png')
      span.right
    .accounts-wrapper
      #search-contact-box
        span.left
        span.center
          input(type:'text')
        span.right
      #my-accounts-box
        ul
          li
            a.account-icon.gtalk(href:"#") 
          li
            a.account-icon.facebook(href:"#")
          li
            a.account-icon.msn(href:"#")
        a#add-contact(href:"#")  
          span.label add account
      #buddy-list-box
        .sections-wrapper
          - each net in ['Chat', 'Gtalk', 'facebook']
            .buddy-list-section(class: net.toLowerCase())
              a.header(href:"#")
                span.header-icon(class: net.toLowerCase())
                span.header-label= net
                span.header-expand
              ul.buddy-list.online
                li.buddy-item.online.layout(style: "display:none;")
                  span.status-icon.online
                  .buddy-info
                    p.name pumiprinsipis
                    p.message el de las garritas
                  img.buddy-photo(src:'/images/buddies/2.jpg')
              ul.buddy-list.away
              ul.buddy-list.idle
              ul.buddy-list.offline
                li.buddy-item.offline.layout
                  span.status-icon
                  .buddy-info
                    p.name pumiprinsipis
                    p.message
                  img.buddy-photo(src:'/images/buddies/1.jpg')
      .accounts-footer
  #right
    #chat-sessions
      .chat-session
        .span-left
        .session-header
          img.photo(src:'/images/photo-2.png')
          .my-info
            span.status-icon
            .info
              p.name mascot one and mascot three
              p.message miauu miauuu miauuuu
            span.network-icon.gtalk
        .span-right
        .chat-buffer-wrapper
          .chat-buffer 
            .message
              img.thumb(src:'/images/buddies/1.jpg') 
              .message-background
                p.single ya mand√© un mensaje desde Node (JS) a MSN
                p.time 12:34